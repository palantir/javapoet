<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN" "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="AvoidEscapedUnicodeCharacters" />
    <suppress checks="AvoidStaticImport" />
    <suppress checks="CyclomaticComplexity" />
    <suppress checks="DesignForExtension" />
    <suppress checks="HiddenField" />
    <suppress checks="LocalVariableName" />
    <suppress checks="MethodLength" />
    <suppress checks="OverloadMethodsDeclarationOrder" />
    <suppress checks="PackageDeclaration" files = "ClassNameNoPackageTest"/>
    <suppress checks="ParameterAssignment" />
    <suppress checks="ParameterName" />
    <suppress checks="SummaryJavadoc" />
    <suppress checks="VisibilityModifier" />
</suppressions>
